class Solution {
public:
    vector<int> singleNumber(vector<int>& nums) {
         unordered_map<int ,int > map;
         vector <int> vec;
        for(int i=0;i<nums.size();i++){
            map[nums[i]]++;
        }
        for(int i=0;i<nums.size();i++){
            if(map[nums[i]]==1){
                vec.push_back(nums[i]);
            }
        }
        return vec;
        
    }
};
